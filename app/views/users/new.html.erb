
<form class="login-signup" action="/users" method="post">
<h1 class="login-signup-header">Sign Up</h1>
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input name="user[email]" type="text" placeholder="email"><br>
  <br>
  <input name="user[password]" type="password" placeholder="password"><br>
  <br>
  <input class="center-block btn btn-primary" type="submit" value="REGISTER" class="btn btn-primary"><br>
</form>
